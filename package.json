{
  "name": "create-soldev-app",
  "description": "Codebase generator for Telkomsel",
  "version": "0.1.9",
  "type": "module",
  "author": "Stradivary",
  "bin": "./bin/run.js",
  "bugs": "https://github.com/Stradivary/create-soldev-app/issues",
  "dependencies": {
    "@inquirer/prompts": "^5.3.8",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-version": "^2.2.11",
    "core-js": "^3.38.0",
    "execa-cjs": "^9.1.1",
    "fs-extra": "^11.2.0",
    "nanospinner": "^1.1.0",
    "tiged": "^2.12.7",
    "tsx": "^4.19.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist/**/*",
    "/oclif.manifest.json",
    "/templates/**/*"
  ],
  "homepage": "https://github.com/Stradivary/create-soldev-app",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "create-soldev-app",
    "dirname": "create-soldev-app",
    "commands": {
      "strategy": "pattern",
      "target": "./dist/commands"
    },
    "plugins": [
      "@oclif/plugin-*"
    ]
  },
  "repository": "Stradivary/create-soldev-app",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "packageManager": "yarn@3.6.4"
}
